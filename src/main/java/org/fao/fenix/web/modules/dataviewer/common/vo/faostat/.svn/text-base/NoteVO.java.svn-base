package org.fao.fenix.web.modules.dataviewer.common.vo.faostat;


import java.util.HashMap;
import java.util.List;

import com.google.gwt.user.client.rpc.IsSerializable;

public class NoteVO implements IsSerializable {

	private String type;
	private String title;
	private String content;
	private Boolean isGroupNote;
	private String groupTitle;
	
	private List<NoteVO> groupNotes;
	
	
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	
	public Boolean getIsGroupNote() {
		return isGroupNote;
	}
	public void setIsGroupNote(Boolean isGroupNote) {
		this.isGroupNote = isGroupNote;
	}
	public String getGroupTitle() {
		return groupTitle;
	}
	public void setGroupTitle(String groupTitle) {
		this.groupTitle = groupTitle;
	}
	public List<NoteVO> getGroupNotes() {
		return groupNotes;
	}
	public void setGroupNotes(List<NoteVO> groupNotes) {
		this.groupNotes = groupNotes;
	}
}
