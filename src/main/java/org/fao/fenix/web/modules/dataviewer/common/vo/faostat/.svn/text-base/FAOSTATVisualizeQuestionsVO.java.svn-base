package org.fao.fenix.web.modules.dataviewer.common.vo.faostat;



import java.util.LinkedHashMap;

import com.google.gwt.user.client.rpc.IsSerializable;

public class FAOSTATVisualizeQuestionsVO implements IsSerializable {
	
	// title of the question
	String title;
	
	// subtitle of the question
	String subtitle;
	
	// description of the question
	String description;
	
	// filename of the questions (containing view settings VOs)
	String filename;
	
	
	// this is for multiple information for the same group of questions
	// not used yet
	String id;
	
	// filename or id of the questions (containing view settings VOs)
	LinkedHashMap<String, FAOSTATVisualizeQuestionsInfoVO> infoVO = new LinkedHashMap<String, FAOSTATVisualizeQuestionsInfoVO>();
	


	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getFilename() {
		return filename;
	}

	public void setFilename(String filename) {
		this.filename = filename;
	}

	public String getSubtitle() {
		return subtitle;
	}

	public void setSubtitle(String subtitle) {
		this.subtitle = subtitle;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public LinkedHashMap<String, FAOSTATVisualizeQuestionsInfoVO> getInfoVO() {
		return infoVO;
	}

	public void setInfoVO(
			LinkedHashMap<String, FAOSTATVisualizeQuestionsInfoVO> infoVO) {
		this.infoVO = infoVO;
	}


	

}
