	public static String getBoxContentLabel(String content, ADAMQueryVO qvo){
		ADAMBoxContent t = ADAMBoxContent.valueOf(content);
		switch(t) {
			case TIMESERIE: return BabelFish.print().TIMESERIE(); 
			case BAR: return BabelFish.print().BAR();
			case BAR_STACK_OR_DONOR: return BabelFish.print().BAR_STACK_OR_DONOR();
			case BAR_WITH_CATEGORIES: return BabelFish.print().BAR_WITH_CATEGORIES();
			case PIE: return BabelFish.print().PIE();
			case SCATTER: return BabelFish.print().SCATTER();
			case BAR_STACK_SECTOR_COMPARISON: 
				if(qvo.isAgricultureFilter() && !qvo.getDatasetSource().equals(ADAMSelectedDataset.ADAM_FPMIS.name()))
					return ADAMLanguage.print().BAR_STACK_SECTOR_COMPARISON_AGRICULTURE();
				else {
					if(qvo.getDatasetSource().equals(ADAMSelectedDataset.ADAM_FPMIS.name()))
							return ADAMLanguage.print().BAR_STACK_OR_COMPARISON();
					else
						return ADAMLanguage.print().BAR_STACK_SECTOR_COMPARISON();
				}	
		}

		return "";
	}
	
