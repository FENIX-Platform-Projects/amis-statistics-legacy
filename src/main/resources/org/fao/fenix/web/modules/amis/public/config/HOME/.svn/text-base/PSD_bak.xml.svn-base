															<ELEMENT label='Domestic use'>60</ELEMENT>
															<ELEMENT label='Closing Stocks'>119</ELEMENT>														</ELEMENTS>								
													</WHERE>
		                                        <GROUPBYS>
								<FIELD>D.year</FIELD>
								<FIELD>E.elementnamee</FIELD>
								<FIELD>D.value</FIELD>
							</GROUPBYS>	
							<SORT type="ASC">
								<FIELD>year</FIELD>
							</SORT>		
						</BASEQUERY>			
