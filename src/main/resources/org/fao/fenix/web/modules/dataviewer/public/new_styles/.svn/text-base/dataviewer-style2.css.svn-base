.textFormat {
	color:#000;
	font-size:12px;
	font-style:normal;
	font-family:Verdana;
	text-align:justify;
	margin:1em;
}

.grey-borders {
 borders: 1px solid red;
 border-color: red;
}

.bullet-list {
    list-style:none;
    margin:0 0 1em 15px;
    padding: 0;
}

.bullet-list li{
    list-style:none;
    line-height:1.2em;
    margin: .4em 0;
    padding: 0 0 0 15px;
    background:url(dataviewer-images/blue_circle_bullet.png) no-repeat 0 4px;
}
.bullet-list li ol{
    list-style:none;
