<VIEW type="Browse By RV - Fertilizers - Trade Value" >

	<TITLE>
<E>Resources - fertilizers (trade value)</E>
  <S>Recursos, fertilizantes (valor de comercio)</S>
  <F>Ressources - engrais (valeur commerciale)</F>
	</TITLE>

	<RESOURCES>
		<DEFAULT>

			<RESOURCE>	
				<RESOURCETYPE>CHART</RESOURCETYPE>
				<OUTPUTTYPE>BAR_WITH_CATEGORIES</OUTPUTTYPE>
				<TITLE>
<E>Imports of top 10 importers (value)</E>
  <S>Importaciones de los 10 principales importadores (valor)</S>
  <F>Les importations du top 10 des importateurs (valeur)</F>
				</TITLE>
				<APPLICABLE_FILTERS>
					<NOT_APPLY_FILTERS>true</NOT_APPLY_FILTERS>
				</APPLICABLE_FILTERS>
				<SIZE>
					<WIDTH>880</WIDTH>
					<HEIGHT>220</HEIGHT>
				</SIZE>
				<BASEQUERY>
					
					<SELECTS>
 						<SELECT lang="true">E.ElementName</SELECT>	
						<SELECT lang="true">A.AreaName</SELECT>		
					</SELECTS>
					<TABLES>		       
						<FROM>Data</FROM>
						<FROM>Element</FROM>
						<FROM>Area</FROM>
						<FROM>Item</FROM>
					</TABLES>
					<WHERE>
						<DOMAINS>
							<DOMAIN label="Fertilizers">RV</DOMAIN>
						</DOMAINS>
						<ELEMENTSLIST>
							<ELEMENTLIST label='Import Value'>2620</ELEMENTLIST>
						</ELEMENTSLIST>		
						<ITEMS>
							<ITEM label='Nitrogenous fertilizers'>3102</ITEM>
							<ITEM label='Phosphate fertilizers'>1375</ITEM>
							<ITEM label='Potash fertilizers	'>1386</ITEM>
						</ITEMS>	
						<YEARS>
							<YEAR>2002</YEAR>
							<YEAR>2003</YEAR>
							<YEAR>2004</YEAR>
							<YEAR>2005</YEAR>
						</YEARS>				
					</WHERE>
					<SORT>
						<FIELD type="aggregation">D.Value</FIELD>
					</SORT>	
			
					<NESTEDLIMIT field="D.AreaCode">10</NESTEDLIMIT>
					
				</BASEQUERY>					
			</RESOURCE>

