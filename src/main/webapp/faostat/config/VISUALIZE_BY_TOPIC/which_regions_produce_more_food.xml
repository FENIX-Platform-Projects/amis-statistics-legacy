<VIEW type="Which region produce more food" >

	<RESOURCES>
		<DEFAULT>

			<!-- Production of cereals -->
			<RESOURCE>	
				<RESOURCETYPE>CHART</RESOURCETYPE>
				<OUTPUTTYPE>TIMESERIE</OUTPUTTYPE>
				<SIZE>
					<WIDTH>490</WIDTH>
					<HEIGHT>300</HEIGHT>
				</SIZE>
				<TITLE>
					<E>Cereal, Total - Production</E>
				</TITLE>
				<DATEQUERY>					<MAX>D.Year</MAX> 
					<TIMESPAN type="year">39</TIMESPAN>
				</DATEQUERY>
				<BASEQUERY>
					<SELECTS>

						<SELECT lang="true">A.AreaName</SELECT>	
						<SELECT>D.Year</SELECT>	
					</SELECTS>
					<TABLES>		       
						<FROM>Data</FROM>
						<FROM>Element</FROM>
						<FROM>Area</FROM>
						<FROM>Item</FROM>
						<FROM>DomainArea</FROM>	
					</TABLES>
					<WHERE>
						<DOMAINS>
							<DOMAIN label="Production">QC</DOMAIN>	
						</DOMAINS>
						<AREAS>
							<AREA label="Africa">5100</AREA>	
							<AREA label="Europa">5400</AREA>	
							<AREA label="America">5200</AREA>	
							<AREA label="Asia">5300</AREA>	
							<AREA label="Oceania">5500</AREA>
							<AREA label="World">5000</AREA>
						</AREAS>
						<ELEMENTS>
							<ELEMENT label='Production'>5510</ELEMENT>
						</ELEMENTS>	
						<ITEMS>
							<ITEM label="Cereals, Total" domain='QC'>1717</ITEM>
						</ITEMS>							
					</WHERE>
					<SORT type="ASC">
					        <FIELD lang="true">A.AreaName</FIELD>
					</SORT>		
					<REGIONAL_LEVEL>TRUE</REGIONAL_LEVEL>
					<COUNTRY_LEVEL>FALSE</COUNTRY_LEVEL>	

				</BASEQUERY>					
			</RESOURCE>

			<!-- Production of cereals PIE -->
			<RESOURCE>	
				<RESOURCETYPE>CHART</RESOURCETYPE>
				<OUTPUTTYPE>PIE</OUTPUTTYPE>
				<SIZE>
					<WIDTH>370</WIDTH>
					<HEIGHT>250</HEIGHT>
				</SIZE>
				<TITLE>
					<E>Cereal, Total - Production</E>
				</TITLE>
				<DATEQUERY>
					<MAX>D.Year</MAX> 
				</DATEQUERY>
				<BASEQUERY>
					
					<SELECTS>
						<SELECT>D.AreaCode</SELECT>	`
						<SELECT lang="true">A.AreaName</SELECT>	
					</SELECTS>
					<TABLES>		       
						<FROM>Data</FROM>
						<FROM>Element</FROM>
						<FROM>Area</FROM>
						<FROM>Item</FROM>
						<FROM>DomainArea</FROM>	
					</TABLES>
					<WHERE>
						<DOMAINS>
							<DOMAIN label="Production">QC</DOMAIN>	
						</DOMAINS>
						<AREAS>
							<AREA label="Africa">5100</AREA>	
							<AREA label="Europa">5400</AREA>	
							<AREA label="America">5200</AREA>	
							<AREA label="Asia">5300</AREA>	
							<AREA label="Oceania">5500</AREA>
						</AREAS>
						<ELEMENTS>
							<ELEMENT label='Production'>5510</ELEMENT>
						</ELEMENTS>
						<ITEMS>
							<ITEM label="Cereals, Total" domain='QC'>1717</ITEM>
						</ITEMS>							
					</WHERE>
					<SORT type="ASC">
					        <FIELD lang="true">A.AreaName</FIELD>
					</SORT>	
					<REGIONAL_LEVEL>TRUE</REGIONAL_LEVEL>
					<COUNTRY_LEVEL>FALSE</COUNTRY_LEVEL>		
		
				</BASEQUERY>					
			</RESOURCE>


