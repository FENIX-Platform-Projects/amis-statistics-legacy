							<SELECTS>
								<SELECT>I.ItemCode</SELECT>	
		 						<SELECT lang="true">I.ItemName</SELECT>	
								<SELECT lang="true">E.ElementName</SELECT>	
								<SELECT lang="true">E.UnitName</SELECT>	
								<SELECT>D.Year</SELECT>
								<SELECT>D.Value</SELECT>
							</SELECTS>
							<TABLES>		       
								<FROM>Data</FROM>
