<StyledLayerDescriptor xmlns="http://www.opengis.net/sld" version="1.0.0" xsi:schemaLocation="http://www.opengis.net/sld http://schemas.opengis.net/sld/1.0.0/StyledLayerDescriptor.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<NamedLayer>
		<Name>ipc_current_phase</Name>
		<UserStyle>
			<Title>IPC current phase</Title>
      <Abstract>IPC current phase - Used on IPC datasets joined with GAUL1</Abstract>
			<FeatureTypeStyle>
				<Rule>
					<Name>secure</Name>
          <Title>1 Generally Food Secure</Title>
					<ogc:Filter xmlns:ogc="http://www.opengis.net/ogc">
						<ogc:PropertyIsEqualTo>
							<ogc:PropertyName>quantity</ogc:PropertyName>
							<ogc:Literal>1</ogc:Literal>
						</ogc:PropertyIsEqualTo>
					</ogc:Filter>
					<PolygonSymbolizer>
						<Fill>
							<CssParameter name="fill">#66FF99</CssParameter>
							<CssParameter name="fill-opacity">1.0</CssParameter>
						</Fill>
						<Stroke>
							<CssParameter name="stroke">#000000</CssParameter>
							<CssParameter name="stroke-opacity">butt</CssParameter>
							<CssParameter name="stroke-width"/>
						</Stroke>
					</PolygonSymbolizer>
				</Rule>
				<Rule>
					<Name>insecure</Name>
          <Title>2 Moderately/Boderline Food Insecure</Title>
					<ogc:Filter xmlns:ogc="http://www.opengis.net/ogc">
						<ogc:PropertyIsEqualTo>
							<ogc:PropertyName>quantity</ogc:PropertyName>
							<ogc:Literal>2</ogc:Literal>
						</ogc:PropertyIsEqualTo>
					</ogc:Filter>
					<PolygonSymbolizer>
						<Fill>
							<CssParameter name="fill">#FFFF99</CssParameter>
							<CssParameter name="fill-opacity">1.0</CssParameter>
						</Fill>
						<Stroke>
							<CssParameter name="stroke">#000000</CssParameter>
							<CssParameter name="stroke-opacity">butt</CssParameter>
							<CssParameter name="stroke-width"/>
						</Stroke>
					</PolygonSymbolizer>
				</Rule>
				<Rule>
					<Name>crisis</Name>
          <Title>3 Acute Food and Livelihood Crisis</Title>
					<ogc:Filter xmlns:ogc="http://www.opengis.net/ogc">
						<ogc:PropertyIsEqualTo>
							<ogc:PropertyName>quantity</ogc:PropertyName>
							<ogc:Literal>3</ogc:Literal>
						</ogc:PropertyIsEqualTo>
					</ogc:Filter>
					<PolygonSymbolizer>
						<Fill>
							<CssParameter name="fill">#FF9900</CssParameter>
							<CssParameter name="fill-opacity">1.0</CssParameter>
						</Fill>
						<Stroke>
							<CssParameter name="stroke">#000000</CssParameter>
							<CssParameter name="stroke-opacity">butt</CssParameter>
							<CssParameter name="stroke-width"/>
						</Stroke>
					</PolygonSymbolizer>
				</Rule>
				<Rule>
					<Name>emergency</Name>
          <Title>4 Humanitarian Emergency</Title>
					<ogc:Filter xmlns:ogc="http://www.opengis.net/ogc">
						<ogc:PropertyIsEqualTo>
							<ogc:PropertyName>quantity</ogc:PropertyName>
							<ogc:Literal>4</ogc:Literal>
						</ogc:PropertyIsEqualTo>
					</ogc:Filter>
					<PolygonSymbolizer>
						<Fill>
							<CssParameter name="fill">#FF3300</CssParameter>
							<CssParameter name="fill-opacity">1.0</CssParameter>
						</Fill>
						<Stroke>
							<CssParameter name="stroke">#000000</CssParameter>
							<CssParameter name="stroke-opacity">butt</CssParameter>
							<CssParameter name="stroke-width"/>
						</Stroke>
					</PolygonSymbolizer>
				</Rule>
				<Rule>
					<Name>catastrophe</Name>
          <Title>5 Famine/Humanitarian Catastrophe</Title>
					<ogc:Filter xmlns:ogc="http://www.opengis.net/ogc">
						<ogc:PropertyIsEqualTo>
							<ogc:PropertyName>quantity</ogc:PropertyName>
							<ogc:Literal>5</ogc:Literal>
						</ogc:PropertyIsEqualTo>
					</ogc:Filter>
					<PolygonSymbolizer>
						<Fill>
							<CssParameter name="fill">#660000</CssParameter>
							<CssParameter name="fill-opacity">1.0</CssParameter>
						</Fill>
						<Stroke>
							<CssParameter name="stroke">#000000</CssParameter>
							<CssParameter name="stroke-opacity">butt</CssParameter>
							<CssParameter name="stroke-width"/>
						</Stroke>
					</PolygonSymbolizer>
				</Rule>
			</FeatureTypeStyle>
		</UserStyle>
	</NamedLayer>
</StyledLayerDescriptor>